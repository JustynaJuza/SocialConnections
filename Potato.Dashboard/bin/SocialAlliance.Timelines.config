<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE socialTimelines [
  <!ELEMENT socialTimelines ANY>
  <!ELEMENT timeline ANY>
  <!ELEMENT youTubeUsers ANY>
  <!ELEMENT youTube EMPTY>
  <!ELEMENT twitterUsers ANY>
  <!ELEMENT twitter EMPTY>
  <!ATTLIST timeline id NMTOKEN #REQUIRED>
  <!ATTLIST timeline name NMTOKEN #IMPLIED>
  <!ATTLIST timeline merged (true | false) "true">
  <!ATTLIST timeline singleUser (true | false) "false">
  <!ATTLIST youTube user NMTOKEN #REQUIRED>
  <!ATTLIST youTube videoResults CDATA "10">
  <!ATTLIST youTube playlistResults CDATA "10">
  <!ATTLIST youTube includePlaylistVideos (true | false) "true">
  <!ATTLIST youTube videosStartIndex CDATA "1">
  <!ATTLIST youTube playlistsStartIndex CDATA "1">
  <!ATTLIST youTube userVideosOrder NMTOKEN "published">
  <!ATTLIST youTube playlistVideosOrder NMTOKEN "position">
  <!ATTLIST youTube includeHowLongSincePublished (true | false) "false">
  <!ATTLIST twitter user NMTOKEN #REQUIRED>
  <!ATTLIST twitter timelineResults CDATA "10">
  <!ATTLIST twitter includeReplies (true | false) "false">
  <!ATTLIST twitter includeRetweets (true | false) "false">
  <!ATTLIST twitter includeHowLongSincePublished (true | false) "false">
]>

